<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="product_category_view_form_inherit_custom_seq" model="ir.ui.view">
        <field name="name">product.category.view.form.inherit.custom.seq</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='first']" position="before">
                <group>
                  <group>
                    <field name="c_code"/>
                  </group>
                </group>
            </xpath>
        </field>
    </record>
  </data>
</odoo>