<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="custom_sequence_for_invoice" model="ir.sequence">
            <field name="name">Customer Invoice Sequence</field>
            <field name="code">account.invoice.sequence</field>
            <field name="prefix">/INV/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>

        <record id="custom_sequence_sale_quotation" model="ir.sequence">
            <field name="name">Sale Quotation Sequence</field>
            <field name="code">sale.quotation.sequence</field>
            <field name="prefix">/QTN/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>

        <record id="custom_sequence_delivery_note" model="ir.sequence">
            <field name="name">Delivery Note Sequence</field>
            <field name="code">delivery.note.sequence</field>
            <field name="prefix">/DN/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>            